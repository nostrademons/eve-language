class List(Iterator):
    def init(seq):
        iter = seq.iter()
        if iter.has_next() 
            then [iter.get(), init(iter.next())]
            else []
    def head(self): Tuple.get(self, 0)
    def tail(self): Tuple.get(self, 1)
    def get(self): self.head()
    def next(self): self.tail()
    def has_next(self): self.next() != []
    def empty(self): add_superclass(List, [])
    def prepend(self, val): [val, self]
